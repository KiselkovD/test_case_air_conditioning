   <string notr="true">/* Основные стили */
* {
    border-color: rgba(128, 128, 128, 0); /* Границы прозрачны */
    background-color: #F9FAFB; /* Светлый фон */
    gridline-color: rgba(200, 200, 200, 0.25); /* Полупрозрачные линии сетки */
    color: #212121; /* Темный текст */
	font-size: 20px
}

QFrame {
    background: #EAECEE; /* Светло-серый фон */
    border: 4px solid #EAECEE; /* Обводка светло-серая */
    border-radius: 10px;
}

/* Поля ввода */
QDoubleSpinBox {
    background-color: #EFEFF1; /* Светло-серый фон */
    color: #212121; /* Темный текст */
    border: 1px solid #BDBEBF; /* Светлая граница */
    padding: 5px;
}

QLineEdit {
    background: #E6E7E8; /* Светло-серый фон */
    border: 5px solid #E6E7E8; /* Светлая граница */
    border-radius: 10px;
}

/* Кнопки */
QPushButton {
    background: #CDD3D9; /* Светло-голубая кнопка */
    border-radius: 5px;
    padding-top: 5px;
    padding-bottom: 5px;
}

QPushButton:pressed {
    background-color: #AABBC1; /* Нажатая кнопка чуть темнее */
    border-style: inset;
}

/* Комбобоксы */
QComboBox {
    background: #DCE1E4; /* Светло-серый фон */
}

/* Вкладки */
QTabWidget::pane {
    border-top: 0px;
    background: #F9FAFB; /* Светлый фон */
	font-size: 20px
}

QTabBar::tab:selected {
    background: #DCDCDC; /* Выделенная вкладка чуть темнее */
    border: 2px solid #ACADAF; /* Слегка темная граница */
    padding-left: 15px;
    padding-right: 15px;
}

QTabBar::tab {
    background: #EAECEE; /* Основной фон вкладок */
}

QTabBar::tab:hover {
    background: #DDE1E3; /* Наведение мыши делает вкладку немного темнее */
}
